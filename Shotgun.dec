	AltFire:
        SHTN A 0
		TNT1 A 0 A_SetCrosshair(41)
		SHTN A 0 A_Giveinventory("GoSpecial",1)
		SHTN A 0 A_JumpIfInventory("Zoomed",1,"NoAim")
