		  A_Setroll(roll+0.2, SPF_INTERPOLATE); // +0.7
	   }
	   SHTZ G 1
	   {
		  A_WeaponReady;
		  A_Setroll(roll-0.6, SPF_INTERPOLATE); 
	   }
	   SHTZ G 4
	   {
		  A_WeaponReady;
		  A_Setroll(roll-0.2, SPF_INTERPOLATE); 
	   }
